{
  "routing_mode": "latency",
  "dns_servers": [
    {"host": "8.8.8.8", "port": 53, "name": "Google DNS"},
    {"host": "1.1.1.1", "port": 53, "name": "Cloudflare DNS"},
    {"host": "9.9.9.9", "port": 53, "name": "Quad9 DNS"}
  ],
  "upstream_proxies": [
    {"type": "socks5", "host": "127.0.0.1", "port": 10808}
  ],
  "interfaces": ["auto"],
  "health_check_interval": 60,
  "accessibility_timeout": 5,
  "dns_timeout": 3,
  "network_timeout": 10,
  "user_validation_timeout": 15,
  "max_concurrent_connections": 100,
  "max_connections_per_runway": 10,
  "success_rate_threshold": 0.5,
  "success_rate_window": 10,
  "log_level": "DEBUG",
  "log_file": "logs/proxy.log",
  "log_max_bytes": 10485760,
  "log_backup_count": 5,
  "proxy_listen_host": "127.0.0.1",
  "proxy_listen_port": 2123
}

